    "react-native-safe-area-context": "^4.2.5",
    "react-native-screens": "^3.13.1",
    "react-redux": "^8.0.2",
    "redux": "4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
